{
  "manifest_version": 3,
  "name": "JobHunt AI Auto-Fill",
  "version": "1.0.0",
  "description": "Auto-fill job applications with your saved profile",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}